# arsenals sepolicy
# read fps
allow system_app vendor_sysfs_graphics:file { read open };
# read battery info
allow system_app vendor_sysfs_battery_supply:dir { search };
allow system_app vendor_sysfs_battery_supply:file { read open getattr };
# read gpu info
allow system_app vendor_sysfs_kgsl:file { read open getattr };
allow system_app vendor_sysfs_kgsl_gpuclk:file { read open getattr };

